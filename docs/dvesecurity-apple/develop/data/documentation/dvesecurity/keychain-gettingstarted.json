/InternetPassword":{"role":"symbol","title":"Keychain.InternetPassword","fragments":[{"kind":"keyword","text":"enum"},{"kind":"text","text":" "},{"kind":"identifier","text":"InternetPassword"}],"abstract":[{"type":"text","text":"A container for internet password items."}],"identifier":"doc:\/\/DVESecurity\/documentation\/DVESecurity\/Keychain\/InternetPassword","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"InternetPassword"}],"url":"\/documentation\/dvesecurity\/keychain\/internetpassword"},"https://developer.apple.com/documentation/security/seckey":{"title":"SecKey","titleInlineContent":[{"type":"text","text":"SecKey"}],"type":"link","identifier":"https:\/\/developer.apple.com\/documentation\/security\/seckey","url":"https:\/\/developer.apple.com\/documentation\/security\/seckey"},"doc://DVESecurity/documentation/DVESecurity/ECCPublicKey":{"role":"symbol","title":"ECCPublicKey","fragments":[{"kind":"keyword","text":"protocol"},{"kind":"text","text":" "},{"kind":"identifier","text":"ECCPublicKey"}],"abstract":[{"type":"text","text":"A type for Elliptic Curve Cryptography (ECC) public keys."}],"identifier":"doc:\/\/DVESecurity\/documentation\/DVESecurity\/ECCPublicKey","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"ECCPublicKey"}],"url":"\/documentation\/dvesecurity\/eccpublickey"},"https://support.apple.com/guide/security/secure-enclave-sec59b0b31ff/web":{"title":"Secure Enclave","titleInlineContent":[{"type":"text","text":"Secure Enclave"}],"type":"link","identifier":"https:\/\/support.apple.com\/guide\/security\/secure-enclave-sec59b0b31ff\/web","url":"https:\/\/support.apple.com\/guide\/security\/secure-enclave-sec59b0b31ff\/web"},"doc://DVESecurity/documentation/DVESecurity/ECCSecureEnclaveKey":{"role":"symbol","title":"ECCSecureEnclaveKey","fragments":[{"kind":"keyword","text":"protocol"},{"kind":"text","text":" "},{"kind":"identifier","text":"ECCSecureEnclaveKey"}],"abstract":[{"type":"text","text":"A type for Secure Enclave Elliptic Curve Cryptography (ECC) keys."}],"identifier":"doc:\/\/DVESecurity\/documentation\/DVESecurity\/ECCSecureEnclaveKey","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"ECCSecureEnclaveKey"}],"url":"\/documentation\/dvesecurity\/eccsecureenclavekey"},"doc://DVESecurity/documentation/DVESecurity/Crypto":{"role":"symbol","title":"Crypto","fragments":[{"kind":"keyword","text":"enum"},{"kind":"text","text":" "},{"kind":"identifier","text":"Crypto"}],"abstract":[{"type":"text","text":"A container for cryptographic types and methods."}],"identifier":"doc:\/\/DVESecurity\/documentation\/DVESecurity\/Crypto","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"Crypto"}],"url":"\/documentation\/dvesecurity\/crypto"},"doc://DVESecurity/documentation/DVESecurity/Keychain":{"role":"symbol","title":"Keychain","fragments":[{"kind":"keyword","text":"enum"},{"kind":"text","text":" "},{"kind":"identifier","text":"Keychain"}],"abstract":[{"type":"text","text":"A container for keychain types and methods."}],"identifier":"doc:\/\/DVESecurity\/documentation\/DVESecurity\/Keychain","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"Keychain"}],"url":"\/documentation\/dvesecurity\/keychain"},"doc://DVESecurity/documentation/DVESecurity/SecKeyConvertible":{"role":"symbol","title":"SecKeyConvertible","fragments":[{"kind":"keyword","text":"protocol"},{"kind":"text","text":" "},{"kind":"identifier","text":"SecKeyConvertible"}],"abstract":[{"type":"text","text":"A type that can be initialized from "},{"type":"reference","isActive":true,"identifier":"https:\/\/developer.apple.com\/documentation\/security\/seckey"},{"type":"text","text":" instances and"},{"type":"text","text":" "},{"type":"text","text":"converted to a "},{"overridingTitleInlineContent":[{"type":"text","text":"SecKey"}],"isActive":true,"type":"reference","identifier":"https:\/\/developer.apple.com\/documentation\/security\/seckey","overridingTitle":"SecKey"},{"type":"text","text":" instance."}],"identifier":"doc:\/\/DVESecurity\/documentation\/DVESecurity\/SecKeyConvertible","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"SecKeyConvertible"}],"url":"\/documentation\/dvesecurity\/seckeyconvertible"},"doc://DVESecurity/documentation/DVESecurity/Crypto/ECC/SecureEnclaveKey":{"role":"symbol","title":"Crypto.ECC.SecureEnclaveKey","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"SecureEnclaveKey"}],"abstract":[{"type":"text","text":"A Elliptic Curve Cryptography private key saved in the Secure Enclave."}],"identifier":"doc:\/\/DVESecurity\/documentation\/DVESecurity\/Crypto\/ECC\/SecureEnclaveKey","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"SecureEnclaveKey"}],"url":"\/documentation\/dvesecurity\/crypto\/ecc\/secureenclavekey"},"doc://DVESecurity/documentation/DVESecurity/RSAPublicKey":{"role":"symbol","title":"RSAPublicKey","fragments":[{"kind":"keyword","text":"protocol"},{"kind":"text","text":" "},{"kind":"identifier","text":"RSAPublicKey"}],"abstract":[{"type":"text","text":"A type representing a RSA public key."}],"identifier":"doc:\/\/DVESecurity\/documentation\/DVESecurity\/RSAPublicKey","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"RSAPublicKey"}],"url":"\/documentation\/dvesecurity\/rsapublickey"},"doc://DVESecurity/documentation/DVESecurity/SecKeyAttributes":{"role":"symbol","title":"SecKeyAttributes","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"SecKeyAttributes"}],"abstract":[{"type":"text","text":"Attributes of a "},{"type":"codeVoice","code":"SecKey"},{"type":"text","text":"."}],"identifier":"doc:\/\/DVESecurity\/documentation\/DVESecurity\/SecKeyAttributes","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"SecKeyAttributes"}],"url":"\/documentation\/dvesecurity\/seckeyattributes"},"doc://DVESecurity/documentation/DVESecurity/DefinesSecKeyClass":{"role":"symbol","title":"DefinesSecKeyClass","fragments":[{"kind":"keyword","text":"protocol"},{"kind":"text","text":" "},{"kind":"identifier","text":"DefinesSecKeyClass"}],"abstract":[{"type":"text","text":"A type that defines to which key class it corresponds in the Security Framework."}],"identifier":"doc:\/\/DVESecurity\/documentation\/DVESecurity\/DefinesSecKeyClass","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"DefinesSecKeyClass"}],"url":"\/documentation\/dvesecurity\/definesseckeyclass"},"doc://DVESecurity/documentation/DVESecurity/Keychain/GenericPassword":{"role":"symbol","title":"Keychain.GenericPassword","fragments":[{"kind":"keyword","text":"enum"},{"kind":"text","text":" "},{"kind":"identifier","text":"GenericPassword"}],"abstract":[{"type":"text","text":"A container for generic password items."}],"identifier":"doc:\/\/DVESecurity\/documentation\/DVESecurity\/Keychain\/GenericPassword","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"GenericPassword"}],"url":"\/documentation\/dvesecurity\/keychain\/genericpassword"},"doc://DVESecurity/documentation/DVESecurity/Keychain/Legacy":{"role":"symbol","title":"Keychain.Legacy","fragments":[{"kind":"keyword","text":"enum"},{"kind":"text","text":" "},{"kind":"identifier","text":"Legacy"}],"abstract":[{"type":"text","text":"A container for legacy file based macOS keychain functions."}],"identifier":"doc:\/\/DVESecurity\/documentation\/DVESecurity\/Keychain\/Legacy","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"Legacy"}],"url":"\/documentation\/dvesecurity\/keychain\/legacy"},"doc://DVESecurity/documentation/DVESecurity/KeychainError":{"role":"symbol","title":"KeychainError","fragments":[{"kind":"keyword","text":"enum"},{"kind":"text","text":" "},{"kind":"identifier","text":"KeychainError"}],"abstract":[{"type":